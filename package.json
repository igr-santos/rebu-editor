{
  "name": "rebu-editor",
  "description": "A editor React based on DraftJS framework",
  "version": "0.1.0",
  "homepage": "https://ourcities.github.io/rebu-editor",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/ourcities/rebu-editor.git"
  },
  "main": "lib/index.js",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "prebuild": "babel src/components --out-dir lib --ignore test.js",
    "create-release-branch": "sh ./scripts/create-release-branch",
    "publish-release": "sh ./scripts/publish-release",
    "deploy-docs": "sh ./scripts/docs",
    "ci-deploy-docs": "sh ./scripts/docs-ci",
    "gh-pages": "REACT_APP_GH_PAGES_PATH='rebu-editor' npm run build"
  },
  "devDependencies": {
    "babel-cli": "^6.14.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.5.0",
    "bootstrap": "^4.0.0-alpha.4",
    "conventional-changelog-cli": "^1.1.1",
    "conventional-recommended-bump": "^0.3.0",
    "dentist": "^1.0.3",
    "draft-js": "^0.9.1",
    "enzyme": "^2.4.1",
    "history": "^4.2.0",
    "react": "^15.4.0",
    "react-addons-css-transition-group": "^15.4.0",
    "react-addons-test-utils": "^15.3.2",
    "react-addons-transition-group": "^15.4.0",
    "react-dom": "^15.4.0",
    "react-prism": "^4.0.0",
    "react-router": "^4.0.0-alpha.3",
    "react-scripts": "0.6.0",
    "reactstrap": "^3.9.0"
  },
  "dependencies": {},
  "peerDependencies": {
    "reactstrap": "^2.0.0 || ^3.0.0"
  },
  "license": "MIT",
  "files": [
    "LICENSE",
    "README.md",
    "CHANGELOG.md",
    "lib"
  ],
  "keywords": [
    "rebu-editor",
    "react",
    "component",
    "components",
    "react-component",
    "reactstrap-component",
    "bootstraip",
    "draftjs",
    "editor"
  ]
}
